#include <iostream>

struct Node {
    Node* left;
    Node* right;
    int data;
};

bool search(Node* node)
{
    if ((node-> right != nullptr) and ((node -> right -> data) > (node -> data)))
    {
        return false;
    }
    if ((node-> left != nullptr) and ((node->left->data) < (node->data)))
    {
        return false;
    }
    if (node == nullptr)
    {
        return true;
    }
    return search((node->left)) and (search(node->right));
}

int main()
{
    setlocale(LC_ALL, "Russian");
    using namespace std;
    Node fl1, fl2, fl3, fl4, fl5;
    fl1.data = 6;
    fl2.data = 8;
    fl3.data = 13;
    fl4.data = 10;
    fl5.data = 7;
    fl1.left = &fl2;
    fl1.right = &fl3;
    fl2.left = &fl4;
    fl2.right = &fl5;
    fl3.left = fl3.right = fl4.left = fl4.right = fl5.left = fl5.right = nullptr;
    if (search(&fl1))
    {
        cout << "True" << endl;
    }
    else
    {
        cout << "False" << endl;
    }
}
